{
  "name": "lightweight-solver",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "author": {
    "email": "stanislav.sakun@ukr.net",
    "name": "Stanislav Sakun"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "clean": "rm -rf dist",
    "prepare": "yarn build",
    "dev": "node --import ./ts-node-register.js --experimental-specifier-resolution=node ./src/index.ts",
    "test": "node --import ./ts-node-register.js --experimental-specifier-resolution=node **/*.test.ts"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "devDependencies": {
    "@types/node": "^24.0.4",
    "prettier": "3.6.1",
    "ts-node": "^10.9.2",
    "ts-node-esm": "^0.0.6",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.0"
  },
  "dependencies": {
    "@lume/kiwi": "^0.4.4"
  }
}
